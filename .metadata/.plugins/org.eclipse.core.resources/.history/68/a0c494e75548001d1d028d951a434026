package thread;

public class Util {
	
	public static void sleep(long milli) {
		try {
			Thread.sleep(milli);
		}
		catch(InterruptedException ex) {
			ex.printStackTrace();
		}
	}
	
	public static void sleep(long milli, long nanoSec) {
		try
		{
			Thread.sleep(milli, nanoSec);
		}
		catch(InterruptedException ex) {
			ex.printStackTrace();
		}
	}
}
