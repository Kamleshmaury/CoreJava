	public static void main(String[] args) {
	try
	{
		Class.forName("com.mysql.jdbc.Driver");
		Connection con = DriverManager.getConnection("", "root", "root");
	}
	catch(Exception ex) {
		System.out.println(ex);
	}
}

}
