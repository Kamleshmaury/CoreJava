package java8;

@FunctionalInterface
interface Addition
{
	int add(int i, int j);
}

public class FunctionalInterfaceDemo {
	public static void main(String[] args) {
		
		new Thread(new Runnable() {
			@Override
			public void run() {
				System.out.println("Form runnable...");
			}
		}).start();
	}

}
