package com.hops1;

import java.io.File;
import org.codehaus.jackson.JsonFactory;

public class JsonParser {
	public static void main(String[] args) {
		JsonFactory factory = new JsonFactory();
		try {
			org.codehaus.jackson.JsonParser parser = factory.createJsonParser(new File("sports.json"));		
			
		}
		catch (Exception e) {
			
			e.printStackTrace();
		}
	}

}
